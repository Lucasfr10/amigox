<div class="row show_group col-md-12">
  <a href="<%= "/events/" + event.id.to_s %>">
    <label class="col-md-12">
      <div class="col-md-4">
        <h4>
          <%= event.name %>
        </h4>
      </div>
      <div class="col-md-8">
        <h5>
          <%= pair.name %>
          <br>
          <small>
            <%= pair.email %>
          </small>
        </h5>
      </div>
    </label>
  </a>
</div>
